<?xml version='1.0' encoding='utf-8'?>
    <widget id="com.gx.inai.calc" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
        <name>Calculadora Riesgos INAI</name>
        <description>
            Aplicacion que muestra el costo de la informacion de un usuario en base a la informacion que proporciona a una entidad.
        </description>
        <author email="f.cisneros.e@gmail.com">
            Fernando Cisneros Escalante Mario Alberto Garcia Torrea
        </author>
        <preference name="phonegap-version" value="5.3.3" />
        <content src="index.html" />
        <access origin="*" />
        <icon src="www/img/icono-app.png" />
        <preference name="xwalkVersion" value="15+" />
        <preference name="xwalkCommandLine" value="--disable-pull-to-refresh-effect" />
        <preference name="xwalkMode" value="embedded" />
        <preference name="xwalkMultipleApk" value="true" />
        <platform name="android">
            <preference name="android-minSdkVersion" value="14" />
            <preference name="StatusBarBackgroundColor" value="#2f6043" />
        </platform>
    </widget>
